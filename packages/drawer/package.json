{
  "name": "@v-c/drawer",
  "type": "module",
  "version": "0.0.2",
  "description": "drawer component for vue",
  "author": "",
  "license": "MIT",
  "keywords": [
    "drawer"
  ],
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./dist/*.js": "./dist/*.js",
    "./dist/*.d.ts": "./dist/*.d.ts",
    "./package.json": "./package.json"
  },
  "main": "index.js",

  "files": [
    "dist",
    "package.json"
  ],
  "scripts": {
    "build": "vite build",
    "prepublish": "pnpm build",
    "test": "vitest run",
    "bump": "bumpp",
    "bump:patch": "bumpp --release patch"
  },
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "dependencies": {
    "@v-c/portal": "workspace:^",
    "@v-c/util": "workspace:^"
  }
}
