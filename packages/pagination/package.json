{
  "name": "@v-c/pagination",
  "type": "module",
  "version": "1.0.0",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./dist/locale/*.js": {
      "types": "./dist/locale/*.d.ts",
      "import": "./dist/locale/*.js",
      "require": "./dist/locale/*.cjs"
    },
    "./dist/locale/*": {
      "types": "./dist/locale/*.d.ts",
      "import": "./dist/locale/*.js",
      "require": "./dist/locale/*.cjs"
    },
    "./locale/*.js": {
      "types": "./dist/locale/*.d.ts",
      "import": "./dist/locale/*.js",
      "require": "./dist/locale/*.cjs"
    },
    "./locale/*": {
      "types": "./dist/locale/*.d.ts",
      "import": "./dist/locale/*.js",
      "require": "./dist/locale/*.cjs"
    },
    "./dist/*": "./dist/*",
    "./package.json": "./package.json"
  },
  "main": "./dist/index.js",
  "files": [
    "dist",
    "package.json"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "vite build",
    "test": "vitest",
    "prepublish": "pnpm build",
    "patch": "bumpp --release patch",
    "bump": "bumpp"
  },
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "dependencies": {
    "@v-c/util": "workspace:^"
  }
}
