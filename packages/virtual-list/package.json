{
  "name": "@v-c/virtual-list",
  "type": "module",
  "version": "1.0.5",
  "description": "Vue Virtual List Component",
  "author": "",
  "license": "MIT",
  "keywords": [
    "vue",
    "virtual-list",
    "vue-component"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
