{
  "name": "@v-c/menu",
  "type": "module",
  "version": "1.0.10",
  "description": "menu ui component for vue",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./dist/*.js": "./dist/*.js",
    "./dist/*.d.ts": "./dist/*.d.ts",
